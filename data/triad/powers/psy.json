                "type": "origins:chance",
                "action": {
                    "type": "origins:execute_command",
                    "command": "effect give @s minecraft:slowness 5 1"
                },
                "chance": 0.33
            }
        },
        "damage_condition": {
            "type": "origins:attacker",
            "entity_condition": {
                "type": "origins:exists"
            }
        }
    },
    
