    "insomnia": {
        "type": "origins:prevent_sleep",
        "message": "Your psychopathy prevents you from sleeping.",
        "set_spawn_point": false
    },
    
    "scare_passives": {
        "type": "origins:action_over_time",
        "interval": 20,
        "entity_action": {
            "type": "origins:execute_command",
            "command": "execute as @e[type=#minecraft:followable_friendly_mobs,distance=..8,type=!minecraft:villager] run damage @s 0.01 minecraft:player_attack"
        }
    },

    "bloodlust_resource": {
        "type": "origins:resource",
        "min": 0,
        "max": 100,
        "start_value": 0,
        "hud_render": {
            "should_render": true,
            "sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar_01.png",
            "bar_index": 5
        },
        "min_action": {
            "type": "origins:execute_command",
            "command": "effect clear @s minecraft:resistance"
        }
    },
    
    "bloodlust_timer": {
        "type": "origins:resource",
        "min": 0,
        "max": 600,
        "start_value": 0,
        "hud_render": {
            "should_render": false
        }
    },
    
    "bloodlust_timer_tick": {
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "*:*_bloodlust_timer",
            "change": 1
        }
    },
    
    "bloodlust_decay": {
        "type": "origins:action_over_time",
        "interval": 1,
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "*:*_bloodlust_resource",
                    "change": -100
                },
                {
                    "type": "origins:change_resource",
                    "resource": "*:*_bloodlust_timer",
                    "change": -600
                }
            ]
        },
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_bloodlust_timer",
            "comparison": ">=",
            "compare_to": 600
        }
    },
        "bloodlust_level_1": {
            "type": "origins:action_over_time",
